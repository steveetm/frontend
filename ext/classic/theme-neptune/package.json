{
       
                                     
      
                                                                                                
                                                                       
       
    "namespace": "Ext",

    "name": "theme-neptune",
    "type": "theme",
    "extend": "theme-neutral",
    "alternateName": [
        "ext-theme-neptune"
    ],

    "framework": "ext",
    "toolkit": "classic",
                   
             
        

    "creator": "Sencha",
    "summary": "Ext JS Neptune Theme",
    "detailedDescription": "The Neptune Theme provides a clean style for Ext JS classic toolkit",

    "version": "6.2.1.167",
    "compatVersion": "6.0.0",
    "format": "1",

    "output": "${workspace.subpkg.prefix}/${package.name}",
    "local": true,

    "sass": {
        "namespace": "Ext",

        "etc": "${package.dir}/sass/etc/all.scss",

        "var": "${package.dir}/sass/var",

        "src": "${package.dir}/sass/src"
    },

    "classpath": "${package.dir}/src",

    "overrides": "${package.dir}/overrides",

    "slicer": {
        "js": [
            {
                "path": "${package.dir}/sass/example/custom.js",
                "isWidgetManifest": true
            }
        ]
    }
}

